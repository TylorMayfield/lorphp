{
  "name": "lorphp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "test:open": "cypress open",
    "test:e2e": "cypress run",
    "test:ci": "start-server-and-test serve http://localhost:8000 test",
    "serve": "php -S localhost:8000 -t public",
    "dev": "php -S localhost:8000 -t public",
    "start": "php -S 0.0.0.0:${PORT:-8080} -t public",
    "migrate": "php bin/lor migrate:run",
    "migrate:rollback": "php bin/lor migrate:rollback",
    "migrate:status": "php bin/lor migrate:status",
    "migrate:create": "php bin/lor migrate:create",
    "migrate:reset": "powershell -Command \"Remove-Item -Path database/schema.json.snapshot -Force -ErrorAction SilentlyContinue; Remove-Item -Path database/migrations/* -Force -ErrorAction SilentlyContinue; php bin/lor generate; php bin/lor db:reset; php bin/seed.php\"",
    "lor:help": "php bin/lor help",
    "lor:event:test": "php bin/lor event:test",
    "lor:make:controller": "php bin/lor make:controller",
    "lor:route:add": "php bin/lor route:add",
    "lor:route:list": "php bin/lor route:list",
    "lor:route:remove": "php bin/lor route:remove",
    "lor:make:model": "php bin/lor make:model",
    "generate": "php bin/generate-all.php",
    "generate:interfaces": "php bin/generate-interfaces.php",
    "generate:model": "php bin/generate-model.php",
    "generate:icons": "node generate-icons.js",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:run": "cypress run",
    "cy:open": "cypress open"
  },
